{
  "name": "react-redux-seed",
  "version": "0.1.0",
  "description": "React Redux Seed",
  "homepage": "https://github.com/r-park/react-redux-seed",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/react-redux-seed.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=5.10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.dist.js --display-chunks --display-modules --progress",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "del-cli ./target",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js --progress",
    "lint": "eslint -c .eslintrc.yml *.js ./src",
    "prebuild": "npm-run-all clean lint test",
    "start": "npm run dev",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-1": "~6.5.0",
    "conventional-changelog-cli": "~1.1.1",
    "cross-env": "~1.0.7",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "2.7.0",
    "eslint-plugin-react": "~4.3.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.15.0",
    "immutable": "~3.7.6",
    "isomorphic-fetch": "~2.2.1",
    "jasmine-core": "~2.4.1",
    "karma": "~0.13.22",
    "karma-chrome-launcher": "~0.2.3",
    "karma-jasmine": "~0.3.8",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "node-sass": "~3.4.2",
    "npm-run-all": "~1.7.0",
    "postcss-loader": "~0.8.2",
    "react": "~15.0.1",
    "react-addons-test-utils": "~15.0.1",
    "react-dom": "~15.0.1",
    "react-redux": "~4.4.2",
    "react-router": "~2.0.1",
    "react-router-redux": "~4.0.2",
    "react-transform-hmr": "~1.0.4",
    "redux": "~3.4.0",
    "redux-thunk": "~2.0.1",
    "reselect": "~2.3.0",
    "sass-loader": "~3.2.0",
    "style-loader": "~0.13.1",
    "webpack": "~1.12.14",
    "webpack-dev-server": "~1.14.1"
  }
}
